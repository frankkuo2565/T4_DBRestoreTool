update tms_op_trn set grp_presale_bgn_date = sysdate-3, grp_presale_end_date = sysdate -2, FIT_PRESALE_BGN_DATE = sysdate - 1 where trn_op_date >= to_date('20180901','yyyyMMdd')
update tms_op_seat_od od set SEAT_STATUS_CODE = (select case when is_saleable = 'Y' then 1 else 2 end from tms_seat_kind_code kind where od.fk_tms_seat_kind_code = kind.pk_tms_seat_kind_code) where trn_op_date >= to_date('20180901','yyyyMMdd')